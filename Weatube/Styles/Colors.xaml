<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="BaseBackgroundBrush" Color="#0d1117"/>
    <Color x:Key="BaseBackgroundColor">#0d1117</Color>

    <SolidColorBrush x:Key="BrightBackgroundBrush" Color="#161b22"/>
    <Color x:Key="BrightBackgroundColor">#161b22</Color>

    <SolidColorBrush x:Key="BaseTextBrush" Color="#a3aab1"/>
    <Color x:Key="BaseTextColor">#a3aab1</Color>

    <SolidColorBrush x:Key="DarkerTextBrush" Color="#6b737f"/>
    <Color x:Key="DarkerTextColor">#6b737f</Color>

    <SolidColorBrush x:Key="BaseButtonBrush" Color="#238636"/>
    <Color x:Key="BaseButtonColor">#238636</Color>

</ResourceDictionary>