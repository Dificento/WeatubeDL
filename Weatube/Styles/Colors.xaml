<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="BaseBackgroundBrush" Color="#260d34"/>
    <Color x:Key="BaseBackgroundColor">#260d34</Color>

    <SolidColorBrush x:Key="BrightBackgroundBrush" Color="#161b22"/>
    <Color x:Key="BrightBackgroundColor">#452459</Color>

    <SolidColorBrush x:Key="BaseTextBrush" Color="#ffffff"/>
    <Color x:Key="BaseTextColor">#ffffff</Color>

    <SolidColorBrush x:Key="DarkerTextBrush" Color="#fe6c90"/>
    <Color x:Key="DarkerTextColor">#fe6c90</Color>

    <SolidColorBrush x:Key="BaseButtonBrush" Color="#fe6c90"/>
    <Color x:Key="BaseButtonColor">#fe6c90</Color>

    <SolidColorBrush x:Key="VideoPanelStartBrush" Color="#d03791"/>
    <Color x:Key="VideoPanelStartColor">#d03791</Color>

    <SolidColorBrush x:Key="ComboBoxDisabledForegroundBrush" Color="#452459" />
    <SolidColorBrush x:Key="ComboBoxDisabledBackgroundBrush" Color="#161b22" />
    <SolidColorBrush x:Key="ComboBoxDisabledBorderBrush" Color="#161b22" />

</ResourceDictionary>